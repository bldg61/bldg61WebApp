<div id="signupCompiler">
  <div id="drop_zone" ondrop="dropHandler(event);" ondragover="dragOverHandler(event);">
    <h3 class="center">Drag signup csv files here</h3>
  </div>

  <h4>
    You have added <span id="numberFilesAdded">0 files</span>
    <a class="btn disabled right teal accent-2 grey-text text-darken-4" id="mungeButton" onclick="signupCompiler(event)">Compile</a>
  </h4>

  <ul class="collection" id="addedFileList"></ul>
</div>



<script type="text/javascript" src="/javascripts/fileDragAndDrop.js"></script>
<script type="text/javascript" src="/javascripts/PapaParse.js"></script>
